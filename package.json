{
  "name": "ngx-notifier",
  "version": "2.0.1",
  "description": "A Simple Notification Service for Angular 5+ Applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "build:package": "sh ./scripts/build.sh",
    "docs": "./node_modules/.bin/compodoc --tsconfig=\"src/tsconfig.app.json\" -n \"NgxNotifier\" --disableCoverage --disablePrivate --disableInternal --disableLifeCycleHooks --disableProtected --hideGenerator --customFavicon=\"src/favicon.ico\" --output=\"docs/\"",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild": "npm run build:package",
    "prebuild": "npm run lint",
    "precommit": "npm run test:single-run",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh",
    "start": "ng serve --aot --open --host 0.0.0.0 --port 4160",
    "serve:docs": "./node_modules/.bin/compodoc --serve --open --output=\"docs/\"",
    "test": "ng test",
    "test:single-run": "npm run lint && ng test --watch false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sibiraj-S/ngx-notifier.git"
  },
  "bugs": {
    "url": "https://github.com/Sibiraj-S/ngx-notifier/issues"
  },
  "homepage": "https://github.com/Sibiraj-S/ngx-notifier",
  "peerDependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "bootstrap": "^4.1.3",
    "rxjs": "~6.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/animations": "^6.1.0",
    "@angular/cli": "~6.2.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@compodoc/compodoc": "^1.1.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.3",
    "codelyzer": "~4.3.0",
    "core-js": "^2.4.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "zone.js": "~0.8.26"
  }
}
