{
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build:demo": "ng build demo --prod",
    "build:lib": "ng build ngx-notifier --prod",
    "build:docs": "compodoc",
    "build": "npm run build:lib && npm run build:demo",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scripts": "eslint ./scripts",
    "ng": "ng",
    "postbuild:lib": "node ./scripts/postbuild.js",
    "prerelease": "npm run build:lib",
    "publish:docs": "npm run build:docs && node ./scripts/publish-docs.js",
    "release": "sh ./scripts/release.sh",
    "serve": "ng serve --aot --open --host 0.0.0.0 --port 4160",
    "serve:docs": "compodoc --serve --watch --open --output=\"docs/\"",
    "test:lib": "ng test ngx-notifier --browsers ChromeHeadless --watch false",
    "test:demo": "ng test demo --browsers ChromeHeadless --watch false",
    "test": "npm run test:lib && npm run test:demo",
    "watch:lib": "ng build ngx-notifier --watch"
  },
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bootstrap": "^4.5.3",
    "chalk": "^4.1.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.12.1",
    "eslint-config-pegasus": "^1.3.10",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
